input[type=checkbox] {
    display: block !important;
    position: absolute;
    opacity: 0;
    cursor: pointer;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    height: 21px;
    z-index: 9;
}
.all-checkbox{
	 display: block !important;
    position: unset !important;
    opacity: 0 !important;
    cursor: pointer !important;
    top: 50% !important;
    left: 50% !important;
    transform: translate(-25%, 0%)!important;
    height: 21px;
    z-index: 9;
	
	
	
}
.checkmarked {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    height: 20px;
    width: 20px;
    border: 1px solid;
}

.checkmarked-button{
    position: unset;
    top: 50%;
    left: 50%;
    transform: translate(-152%, 0%);
    height: 20px;
    width: 20px;
    border: 1px solid;
}
/* When the checkbox is checked, add a blue background */
input:checked ~ .checkmarked-button{
  background-color: #ff8f09;
  border: 2px solid #ef880b!important;
}
input:checked ~ .checkmarked {
  background-color: #ff8f09;
  border: 2px solid #ef880b!important;
}

/* Create the checkmark/indicator (hidden when not checked) */
.checkmarked:after {
  content: "";
  position: absolute;
  display: none;
}
.checkmarked-button:after {
  content: "";
  position: absolute;
  display: none;
}

/* Show the checkmark when checked */
input:checked ~ .checkmarked:after {
  display: block;
}

input:checked ~ .checkmarked-button:after {
  display: block;
}

/* Style the checkmark/indicator */
.checkmarked:after {
  left: 5px;
    top: 0px;
    width: 7px;
    height: 14px;
  border: solid white;
  border-width: 0 3px 3px 0;
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  transform: rotate(45deg);
}
.checkmarked-button:after {
  left: 5px;
    top: 0px;
    width: 7px;
    height: 14px;
  border: solid white;
  border-width: 0 3px 3px 0;
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  transform: rotate(45deg);
}
